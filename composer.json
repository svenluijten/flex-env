{
    "name": "sven/flex-env",
    "description": "Edit your .env file in Laravel directly from the command line.",
    "keywords": [
        "flexible",
        "env",
        "laravel",
        "artisan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "contact@svenluijten.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/framework": "^8.22",
        "sven/file-config": "^3.1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.4",
        "phpunit/phpunit": "^8.3 || ^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Sven\\FlexEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\FlexEnv\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\FlexEnv\\ServiceProvider"
            ]
        }
    }
}
