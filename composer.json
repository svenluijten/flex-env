{
    "name": "sven/flex-env",
    "description": "Edit your .env file in Laravel directly from the command line.",
    "keywords": ["flexible", "env", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sven\\FlexEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\FlexEnv\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\FlexEnv\\ServiceProvider"
            ]
        }
    }
}
